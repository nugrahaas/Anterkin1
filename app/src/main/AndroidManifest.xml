<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.anterkin">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity android:name=".ShopReport" />
        <activity android:name=".BoxReport" />
        <activity android:name=".RideReport" />
        <activity android:name=".FoodReport" />
        <activity android:name=".AnterShop" />
        <activity android:name=".AnterBox" />
        <activity android:name=".AnterRide" />
        <activity android:name=".AnterFood" />
        <activity android:name=".MainActivity">

        </activity>
    </application>

</manifest>